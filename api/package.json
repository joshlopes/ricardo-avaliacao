{
  "name": "api",
  "version": "0.2.4",
  "description": "PoC for school grades",
  "author": "josluis.lopes@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoshLopes/ricardo-avaliacao.git"
  },
  "bugs": {
    "url": "https://github.com/JoshLopes/ricardo-avaliacao/issues"
  },
  "keywords": [],
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'npx ts-node' src/index.ts",
    "check:type": "tsc --noEmit",
    "test": "npx jest --silent",
    "lint": "npm run check:type && npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "build": "tsc -p .",
    "prisma:generate": "prisma generate",
    "dev:db:drop": "npx prisma migrate reset --force",
    "dev:db:push": "npx prisma db push",
    "dev:setup": "npm run dev:db:drop && npm run dev:db:push",
    "test:db:drop": "env-cmd -f .env.test npx prisma migrate reset --force",
    "test:db:push": "env-cmd -f .env.test npx prisma db push",
    "test:setup": "npm run test:db:drop && npm run test:db:push"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "axios": "^1.7.9",
    "bcrypt": "^6.0.0",
    "bignumber.js": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "inversify": "^7.5.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mathjs": "^14.5.2",
    "npid": "^0.4.0",
    "prisma": "^6.9.0",
    "promptly": "^3.2.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.15.31",
    "@types/promptly": "^3.0.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/yargs": "^17.0.33",
    "env-cmd": "^10.1.0",
    "eslint-config-love": "^120.0.0",
    "jest": "^30.0.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "optionalDependencies": {
    "argon2": "^0.41.1"
  }
}
